<h2>Movie</h2>
<ul>
    <li>Title:{{title}}</li>
    <li>Genre:{{genre}}</li>
    <li>Plot:{{plot}}</li>
    {{#each cast}}
    <li>Name:{{this.name}}</li>
    <li>Occupation:{{this.occupation}}</li>
    <li>CatchPhrase:{{this.catchPhrase}}</li>
    {{/each}}
</ul>

<form action="/movies/{{id}}/delete" method="POST">

<button type="submit">Delte movie</button>
</form>